<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wecraw.treatyourself">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LogEventActivity" />
        <activity android:name=".activities.NewEventActivity" />
        <activity android:name=".activities.EventDetailActivity" />
        <activity
            android:name=".activities.WelcomeActivity"
            android:noHistory="true" />
        <activity android:name=".activities.UserProfileActivity" />
        <activity android:name=".activities.UserLogActivity" />
        <activity android:name=".activities.TodoListActivity" />
        <activity android:name=".activities.NewTodoActivity" />
        <activity
            android:name=".activities.BoostedChoiceActivity"
            android:noHistory="true" />
        <activity android:name=".BoostActivity" />
        <activity android:name=".activities.SettingsActivity" />
        <activity android:name=".activities.TermsOfUseActivity" />
        <activity android:name=".activities.AboutActivity" />
        <activity android:name=".activities.ResetActivity" />
        <activity android:name=".activities.EarnPointsActivity" />
        <activity android:name=".activities.SpendPointsActivity"></activity>
    </application>

</manifest>